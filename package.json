{
  "name": "playwright_project",
  "version": "1.0.0",
  "description": "end-to-end testing with PlayWright",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "npx playwright test --headed",
    "test:chrome": "npx playwright test --project=chromium",
    "test:debug": " npx playwright test --debug",
    "amazonItem": "npx playwright test tests/amazonItem.spec.js --headed",
    "sandbox": "npx playwright test tests/sandbox.spec.js --headed",
    "report": "npx playwright show-report",
    "viewTrace": "npx playwright show-trace trace.zip"
  },
  "author": "Maria Markel",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.19.1"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "playwright": "^1.19.2"
  }
}
